        var root_path = '/';

        $(document).ready(function () {
            $('#btnSchoolSearch').click(function () { schoolSearch(); })
        });

        //查询
        function schoolSearch() {
            var keyword = $("#keyword").val().replace(MVC_ROUTE_FILTER_REG, "");
            var para = $.trim(keyword) == "" ? "" : "&key=" + keyword;
            if (para.substr(0, 1) == "&") para = para.substr(1, para.length - 1)

            var url = root_path + "Home/Maps" + ($.trim(para) == "" ? "" : "?") + para;
            window.location.href = url;
        }
    </script>
